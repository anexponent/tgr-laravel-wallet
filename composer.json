{
    "name": "anexponent/tgr-laravel-wallet",
    "description": "Easy to use virtual wallet for your app",
    "keywords": [
        "anexponent",
        "depsimon",
        "laravel-wallet",
        "virtual",
        "currency",
        "credits",
        "wallet"
    ],
    "homepage": "https://github.com/anexponent/laravel-wallet",
    "license": "MIT",
    "authors": [
        {
            "name": "Azeez Adesina",
            "email": "azeezadesina@yahoo.com",
            "role": "Modifier"
        }
    ],
    "require": {
        "php": "^7.0 || ^8.0",
        "illuminate/database": "~5.6|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Depsimon\\Wallet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Depsimon\\Wallet\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depsimon\\Wallet\\WalletServiceProvider"
            ],
            "aliases": {
                "Wallet": "Depsimon\\Wallet\\WalletFacade"
            }
        }
    }
}
